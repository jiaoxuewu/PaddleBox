                                    int out_dim_size, 
                                    int dim_start_offset,
                                    int cur_y_index,
                                    __global_ptr__ T* cur_y) {
        // use cvm
        // skip click offset + 1
        sum_result[dim_start_offset] = (float)log(sum_result[0] + 1);

        mfence();
        LM2GM_ASYNC(sum_result + dim_start_offset, cur_y + cur_y_index, out_dim_size * sizeof(T));
    }
};

