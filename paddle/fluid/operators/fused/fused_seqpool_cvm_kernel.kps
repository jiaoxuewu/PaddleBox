                                bool fix_ctr_to_click,
                                __global_ptr__ T* cur_x,
                                __global_ptr__ T* cur_y) {
        // first: sum pool
        for (int i = batch_start; i < batch_end; i += max_seq_len) {
            // int len = min<int64_t>(batch_end - i, max_seq_len);
            int len = min(batch_end - i, max_seq_len);
            if (len <= 0)
                continue;
