template <typename T, typename TID>
static int sequence_sum_pool_cvm_with_conv_xpu2_wrapper(xpu::Context* ctx,
                        T** x_vec,
                        T** y_vec,
                        TID* lods_vec,
                        uint32_t batch,
                        uint32_t dim,
